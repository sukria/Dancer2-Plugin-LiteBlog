<div class="container">
<article class="main-content">
<h1>[% page_title %]</h1>

<ul class="tags">
[% IF meta %]
    [% FOREACH m IN meta %]
      [% IF m.link %]
      <li class="clickable"><a href="[% m.link %]/">[% m.label %]</a></li>
      [% ELSE %]
      <li>[% m.label %]</li>
      [% END %]
    [% END %]
  [% END %]
</ul>
  [% content %]
  <!-- </div> -->

</article> 
</div> 
